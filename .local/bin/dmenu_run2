#!/bin/sh
programs=brave:firefox:firefox_sb:zotero:thunderbird:calibre:slack:signal-desktop:telegram-desktop:spotify:attack_vpn:pavucontrol:blueman:blueman-manager:easyeffects:peek:simplescreenrecorder:fiji:inkscape:chbg:dmount:dumount:dmenuunicode:timer
programs=$programs:reboot:poweroff
echo $programs | sed 's/:/\n/g' | dmenu -p "RUN" "$@" | ${SHELL:-"/bin/sh"} &
