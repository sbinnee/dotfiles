#!/bin/sh
programs=st:alacritty:brave:firefox:firefox_sb:zotero:thunderbird:calibre:filezilla:signal-desktop:telegram-desktop:spotify:attack_vpn:gpclient:pavucontrol:qpwgraph:blueman:easyeffects:peek:simplescreenrecorder:fiji:inkscape:watch_later:chbg:dmount:dumount:dmenuunicode:timer
programs=$programs:reboot:poweroff
echo $programs | sed 's/:/\n/g' | dmenu -p "RUN" "$@" | ${SHELL:-"/bin/sh"} &
# echo $programs | sed 's/:/\n/g'
